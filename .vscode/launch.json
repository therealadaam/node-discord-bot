	"version": "0.2.0",
	"configurations": [
		{
			"name": "Attach to nodemon",
			// "processId": "${command:PickProcess}",
			"port": 9229,
			"restart": true,
			"request": "attach",
			"skipFiles": [
				"${workspaceFolder}/node_modules/**/*.js",
				"<node_internals>/**"
			],
			"type": "pwa-node"
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/bot.js"
		}
	]
}
